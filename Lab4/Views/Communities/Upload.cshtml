
@{
    ViewData["Title"] = "Create";
}

<h1>Upload</h1>

<h4>Upload an answer</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Upload" enctype="multipart/form-data">
            <div class="form-group">
       
                <div class="btn btn-sm float-left">
                    <input name="ads" type="file">
                </div>
            </div>

            <div class="form-group">

                <button name="upload" class="btn btn-primary">Upload Now</button>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
